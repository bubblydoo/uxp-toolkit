{
  "name": "@bubblydoo/eslint-plugin-uxp",
  "type": "module",
  "version": "0.0.4",
  "author": "Hans Otto Wirtz <hansottowirtz@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/bubblydoo/uxp-toolkit/tree/main/packages/eslint-plugin-uxp",
  "repository": {
    "type": "git",
    "url": "https://github.com/bubblydoo/uxp-toolkit.git"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --outDir dist --target node18",
    "typecheck": "tsc --noEmit",
    "docs-json": "typedoc --json docs.json"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0"
  },
  "devDependencies": {
    "@bubblydoo/tsconfig": "workspace:*",
    "@types/eslint": "^9.6.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
